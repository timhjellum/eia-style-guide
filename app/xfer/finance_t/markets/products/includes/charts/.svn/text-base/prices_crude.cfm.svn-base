			name: 'Dubai'
		};		
		
			
	$(window).load(function() {
		$.get( 'data.cfm?chart=prices1', function(data) {
			var vals = data.DATA,
					now = new Date(),
					tmp;
			
			$.each(vals, function(lineNo, items) {
				//Date must be in format month / day / year -- if no day, then default is 1
				var date = Date.parse(items[1]+ '/1/' + items[0]),
					wti = parseFloat(items[2]) || 0,
					brent = parseFloat(items[3]) || 0;
					mars = parseFloat(items[4]) || 0,
					tapis = parseFloat(items[5]) || 0,
					dubai = parseFloat(items[6]) || 0;


				if ( !isNaN( date ) ) {
					series1.data.push( [date, wti] );
					series2.data.push( [date, brent] );
					series3.data.push( [date, mars] );
					series4.data.push( [date, tapis] );
					series5.data.push( [date, dubai] );
				}
			});
			
			opts_prices_crude.series.push(series1);
			opts_prices_crude.series.push(series2);
			opts_prices_crude.series.push(series3);
			opts_prices_crude.series.push(series4);
			opts_prices_crude.series.push(series5);

			chart = new Highcharts.Chart(opts_prices_crude);
				

			
			
			chart.renderer.image('../..//global/images/icons/icon_interactive.gif', 625, 23, 69, 17)
			.add();
			
		},'json');
		
	});
